
#include <stdio.h>
#include<string.h>

int main()
{
    char strname[100],strsurname[100];
    fgets(strname,100,stdin);
    fgets(strsurname,100,stdin); 
    printf("First string:%s",strname);
    printf("Second string:%s",strsurname);
    strcat(strname,strsurname);
    printf("Concatenated String:%s",strname);
   
  
    

    return 0;
}
